<?xml version="1.0"?>
<launch>
<!--         <master auto="true"/> -->
<!--         <param name="/use_sim_time" value="true" /> -->
  <arg name="debug" default="false"/>

  <arg name="config" default="rrt_params_23.yaml"/>
  <arg name="map" default="test_map_fork_4.yaml"/>
  <arg name="problem" default="problem_complex_scenario.yaml"/>
  <arg name="velocity_file" default=""/>  <!--sin velocidaes, tomare random commands -->
  <!--<arg name="output_file" default="$(find siar_planner)/output_files/rrt_params_1.txt"/>-->

        
  <!--Start the map server for the path planner-->
  <node name="map_server" pkg="map_server" type="map_server" args="$(find siar_planner)/test/$(arg map)" output="screen"/>
  
  <node name="birrt_test" pkg="siar_planner" type="birrt_test" output="screen" unless="$(arg debug)">
    <remap from="/altitude_map" to="/map"/>
    <rosparam file="$(find siar_planner)/test/$(arg config)" command="load"  /> 
    <rosparam file="$(find siar_planner)/test/$(arg problem)" command="load"  /> 

    <param name="velocity_file" value="$(find siar_planner)/test/$(arg velocity_file)" />
    <param name="output_file" type="string" value="$(find siar_planner)/output_files/birrt_params_in_complex_scenario_1.txt" />

  </node>

  
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find siar_planner)/test/test.rviz"/>
</launch>
